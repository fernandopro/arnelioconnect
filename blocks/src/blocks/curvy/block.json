{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tarokina/curvy",
	"version": "0.1.0",
	"title": "Curvy",
	"category": "widgets",
	"icon": "smiley",
	"description": "Muestra un panel curvo con un t√≠tulo y un texto",
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"link": false,
			"text": false
		}
	},
	"attributes": {
		"enableTopCurve": {
			"type": "boolean",
			"default": true
		},
		"enableSelectTarot": {
			"type": "boolean",
			"default": true
		},
		"selectedTarot": {
            "type": "string",
            "default": "tarot1"
        }
	},
	"textdomain": "arnelioconnect",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
